    string $cone[] = `polyCone -r 1 -h 2 -sx 20 -sy 1 -sz 0 -ax 0 1 0 -rcp 0 -cuv 3 -ch 1`;
    select $cone;
    move 0 1 0;
    rename $cone[0] "myConeJeff";
    select myConeJeff;
    duplicate;
    rotate 180 0 0;


// get everything selected
    string $sel[] = `ls -sl`;
    //create our loop
    for ($each in $sel)
    {
        //do something - use $each to identify objects
        select $each;
        duplicate -rr;
        rotate -r 0 18 0;
        move -r 0 1 0;
    }


// Basic UI Example: My Amazing UI

// Delete any existing window with this name
if (`window -exists myAmazingUI`) 
{
    deleteUI -window myAmazingUI;
}

// Define a procedure for "rotateCube"
global proc rotate() 
{
    string $sel[] = `ls -sl`;
    for ($each in $sel)
    {
        rotate -r 0 18 0;
    }
}

// Build the window
window -title "My Amazing UI" -widthHeight 200 100 myAmazingUI;
columnLayout -adjustableColumn true -rowSpacing 10;
    button -label "Rotate" -command "rotate()";
setParent ..;

// Show the window
showWindow myAmazingUI;
