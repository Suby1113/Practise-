    string $cone[] = `polyCone -r 1 -h 2 -sx 20 -sy 1 -sz 0 -ax 0 1 0 -rcp 0 -cuv 3 -ch 1`;
    select $cone;
    move 0 1 0;
    rename $cone[0] "myConeJeff";
    select myConeJeff;
    duplicate;
    rotate 180 0 0;
    // get everything selected
    string $sel[] = `ls -sl`;
    //create our loop this makes the selected objects duplicate and move one space up on the Y axis it can also rotate 180 if wanted
    for ($each in $sel)
    {
        //do something - use $each to identify objects
        select $each;
        duplicate;
        //rotate 180 0 0;
        move -r 0 1 0;
    }
